game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "1" then


   Player = game:GetService("Players").LocalPlayer.Name
   AnimationId = "6108512485"
   local Anim = Instance.new("Animation")
   Anim.AnimationId = "rbxassetid://"..AnimationId
   local k = game.Players[Player].Character.Humanoid:LoadAnimation(Anim)
   k:Play() --Play the animation
   k:AdjustSpeed(0.8)
  
wait(0.6)
local A_1 = game:GetService("Workspace")[TheUser].Suit.UpperTorso.Main
local A_2 = game:GetService("Workspace")[TheUser].UpperTorso
local Event = game:GetService("ReplicatedStorage").Events.WeldPiece
Event:FireServer(A_1, A_2)
 

wait(0.01)

