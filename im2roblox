local LocalPlayer = game.Players.LocalPlayer
local Players = game:GetService("Players")


Players.PlayerAdded:Connect(function(player)
  if player.Name == "tipthedripper" or player.Name=="XxOmniversalxX" or player.Name=="bifungiji" or player.Name=="CertifiedComrade" then
wait(3)
-- Script generated by R2Sv2
-- R2Sv2 developed by Luckyxero
 
local A_1 = "/w tipthedripper Im Using Cylindrical"
local A_2 = "All"
local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
Event:FireServer(A_1, A_2)

local A_1 = "/w CertifiedComrade Im Using Cylindrical"
local A_2 = "All"
local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
Event:FireServer(A_1, A_2)

local A_1 = "/w bifungiji Im Using Cylindrical"
local A_2 = "All"
local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
Event:FireServer(A_1, A_2)

local A_1 = "/w XxOmniversalxX Im Using Cylindrical"
local A_2 = "All"
local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
Event:FireServer(A_1, A_2)

  end
end)

















































game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "c" then
wait(7)
for i,v in pairs(game.Players.LocalPlayer.Character.Suit.RightFoot:GetDescendants()) do
   if v.name == "Union" then
       v:Destroy()
        end
    end
end
end)

game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "c" then
Mark = PSuit.Text
if PSuit.Text == 'Cylindrical' then
     local a = require(game:GetService("ReplicatedStorage").Modules.SuitData);
a["Mark 2"].Efficiency = 9e9;
a["Mark 2"].FlightSpeed = 800
a["Mark 2"].MaxHeight = 9e9;
a["Mark 2"].RepulsorCooldown = 0.1
local A_1 = "Mark 2"
local Event = game:GetService("ReplicatedStorage").Events.RequestSuit
Event:InvokeServer(A_1)
wait(0.5)
local A_1 = 'Mark 2'
local Event = game:GetService("ReplicatedStorage").Events.RequestSuit
Event:InvokeServer(A_1)
wait(3)

while true do
wait(0.1)


for i,v in pairs(game.Players.LocalPlayer.Character.Suit:GetDescendants()) do
   if v.name == "Union" then
        if v ~= game.Players.LocalPlayer.Character.Suit.Helmet.Union then
        v:Destroy()
        end
    end
end
TheUser = game:GetService("Players").LocalPlayer.Name
game.Players.LocalPlayer.Character.Suit.RightUpperArm.Part:Destroy()
game.Players.LocalPlayer.Character.Suit.LeftUpperArm.Part:Destroy()
game:GetService("Workspace")[TheUser].Suit.Mask.LowerFace:Destroy()
game:GetService("Workspace")[TheUser].Suit.Mask.Eyes:Destroy()
game:GetService("Workspace")[TheUser].Suit.Mask.Main.ThrusterGlowAttachment:Destroy()
game:GetService("Workspace")[TheUser].Suit.Mask.Main.ThrusterGlowAttachment:Destroy()
game:GetService("Workspace")[TheUser].Suit.RightFoot.Main.ThrusterGlowAttachment:Destroy()
game:GetService("Workspace")[TheUser].Suit.LeftFoot.Main.ThrusterGlowAttachment:Destroy()
game:GetService("Workspace")[TheUser].Suit.RightFoot.Thruster:Destroy()
game:GetService("Workspace")[TheUser].Suit.LeftFoot.Thruster:Destroy()
game:GetService("Workspace")[TheUser].Suit.LeftHand.Thruster:Destroy()
game:GetService("Workspace")[TheUser].Suit.RightHand.Thruster:Destroy()
game:GetService("Workspace")[TheUser].Suit.RightHand.Main.ThrusterGlowAttachment:Destroy()
game:GetService("Workspace")[TheUser].Suit.LeftHand.Main.ThrusterGlowAttachment:Destroy()
game:GetService("Workspace")[TheUser].Suit.UpperTorso.Main.ThrusterGlowAttachment:Destroy()
game:GetService("Workspace")[TheUser].Suit.UpperTorso.Reactor:Destroy()

