Bowl = true

ToggleMask.Name = "ToggleMask"
ToggleMask.Parent = Music
ToggleMask.BackgroundColor3 = Color3.fromRGB(54, 54, 54)
ToggleMask.BorderColor3 = Color3.fromRGB(0, 0, 0)
ToggleMask.BorderSizePixel = 5
ToggleMask.Position = UDim2.new(-0.0036764706, 0, 0.169204742, 0)
ToggleMask.Size = UDim2.new(0, 272, 0, 50)
ToggleMask.Font = Enum.Font.SciFi
ToggleMask.Text = "Toggle Mask"
ToggleMask.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleMask.TextSize = 40.000
ToggleMask.MouseButton1Down:connect(function()
if Bowl == true then 
TheUser = game:GetService("Players").LocalPlayer.Name
game:GetService("Workspace")[TheUser].Suit.Mask.fakeHead.CanCollide = true
game:GetService("Workspace")[TheUser].Suit.Helmet.Helmet.CanCollide = true
local A_1 = game:GetService("Workspace")[TheUser].Suit.Mask
local Event = game:GetService("ReplicatedStorage").Events.CallPiece
Event:FireServer(A_1)
wait(0.3)
game:GetService("Workspace")[TheUser].Suit.Mask.fakeHead.CFrame = CFrame.new(1365, -8, -18)
local A_1 = game:GetService("Workspace")[TheUser].Suit.Helmet
local Event = game:GetService("ReplicatedStorage").Events.CallPiece
Event:FireServer(A_1)

wait(0.3)
game:GetService("Workspace")[TheUser].Suit.Helmet.Helmet.CFrame = CFrame.new(1365, -8, -18)


Bowl = false
else if Bowl == false then
        TheUser = game:GetService("Players").LocalPlayer.Name
local A_1 = game:GetService("Workspace")[TheUser].Suit.Helmet.HelmetMain
local A_2 = game:GetService("Workspace")[TheUser].Head
local Event = game:GetService("ReplicatedStorage").Events.WeldPiece
Event:FireServer(A_1, A_2)
wait(0.2)
local A_1 = game:GetService("Workspace")[TheUser].Suit.Mask.Main
local A_2 = game:GetService("Workspace")[TheUser].Head
local Event = game:GetService("ReplicatedStorage").Events.WeldPiece
Event:FireServer(A_1, A_2)
Bowl = true
end
end
end)

